          <TableColumn fx:id="colProductoId" prefWidth="74.0" text="producto Id" />
          <TableColumn fx:id="colNombre" prefWidth="53.0" text="nombre" />
            <TableColumn fx:id="colDescripcion" prefWidth="71.0" text="descripcion" />
            <TableColumn fx:id="colStock" prefWidth="43.0" text="stock" />
            <TableColumn fx:id="colPresioUnitario" prefWidth="92.0" text="presio unitario" />
            <TableColumn fx:id="colPresioMayor" prefWidth="113.0" text="presio por mayor" />
            <TableColumn fx:id="colCompra" prefWidth="52.0" text="compra" />
            <TableColumn fx:id="colImagen" prefWidth="56.0" text="imagen" />
            <TableColumn fx:id="colDistribuidor" minWidth="0.0" prefWidth="90.0" text="Distribuidor " />
